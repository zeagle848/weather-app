{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-shadow": "off",
    "linebreak-style": 0,
    "quotes": 0,
    "comma-dangle": "off",
    "no-alert": "off",
    "no-restricted-globals": "off",
    "import/prefer-default-export": "off"
  }
}
